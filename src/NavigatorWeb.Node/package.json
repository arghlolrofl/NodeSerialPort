{
  "name": "navigator-web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "postinstall": "typings install",
    "start": "npm run build && node ./bin/www",
    "build": "tsc --project ./src",
    "rebuild": "npm run clean && npm run build",
    "clean": "RD /S /Q \"./dist\" && MD \"./dist\"",
    "clean-libs": "RD .\\public\\lib\\ /s /q && MD .\\public\\lib\\",
    "copy-libs": "npm run clean-libs && xcopy .\\node_modules\\core-js\\client\\shim.min.js .\\public\\lib /YC && xcopy .\\node_modules\\zone.js\\dist\\zone.js .\\public\\lib /YC && xcopy .\\node_modules\\reflect-metadata\\Reflect.js .\\public\\lib /YC && xcopy .\\node_modules\\systemjs\\dist\\system.src.js .\\public\\lib /YC",
    "test": "npm run build && jasmine-node --test-dir ./dist/spec",
    "lint": "tslint -c .\\src\\tslint.json .\\src\\**\\*.ts",
    "tsc": "tsc",
    "typings": "typings"
  },
  "description": "NavigatorWeb",
  "author": {
    "name": "Irmischer",
    "email": "h.irmischer@francotyp.com"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.5",
    "@angular/compiler": "2.0.0-rc.5",
    "@angular/core": "2.0.0-rc.5",
    "@angular/forms": "0.3.0",
    "@angular/http": "2.0.0-rc.5",
    "@angular/platform-browser": "2.0.0-rc.5",
    "@angular/platform-browser-dynamic": "2.0.0-rc.5",
    "@angular/router": "3.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.2",
    "@angular/upgrade": "2.0.0-rc.5",
    "body-parser": "~1.8.1",
    "cookie-parser": "~1.3.3",
    "core-js": "^2.4.0",
    "debug": "~2.0.0",
    "eventemitter3": "^1.2.0",
    "express": "^4.9.8",
    "morgan": "~1.3.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "serialport": "^4.0.1",
    "serve-favicon": "~2.1.3",
    "socket.io": "^1.4.8",
    "systemjs": "0.19.27",
    "ws": "^1.1.1",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "jasmine": "^2.5.0",
    "socket.io-client": "^1.4.8"
  }
}
